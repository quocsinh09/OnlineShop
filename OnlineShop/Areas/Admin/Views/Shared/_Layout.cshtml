

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">


    <!-- Title Page-->
    <title>@ViewBag.Title</title>

    <!-- Fontfaces CSS-->
    <link href="/Assets/css/font-face.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="/Assets/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!--/Assets/vendor CSS-->
    <link href="/Assets/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="/Assets/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="/Assets/css/theme.css" rel="stylesheet" media="all">
    <link href="/Content/PagedList.css" rel="stylesheet" type="text/css" />

</head>

<body >
    <div class="page-wrapper">
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="@Url.Action("Index", "Home", new { area = "Admin" })">
                            <img src=" /Assets/images/icon/logo-blue.png" alt="Admin" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li>
                            <a href="@Url.Action("Index", "Home", new { area = "Admin" })">
                                <i class="fas fa-globe"></i>Tổng quan
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Product", new { area = "Admin" })">
                                <i class="fas fa-file-text"></i>Sản phẩm
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Index", ((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).TypeOfAccount == 2 ? "User" : "Home", new { area = "Admin" })">
                                <i class="fas fa-chart-bar"></i>Tài khoản
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Order", new { area = "Admin" })">
                                <i class="fas fa-table"></i>Đơn hàng
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="@Url.Action("Index", "Home", new { area = "Admin" })">
                    <img src="@Url.Content("~/Assets/images/icon/logo-blue.png")" alt="Admin" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">

                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a href="@Url.Action("Index", "Home", new { area = "Admin" })">
                                <i class="fas fa-globe"></i>Tổng quan
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Product", new { area = "Admin" })">
                                <i class="fas fa-archive"></i> Sản phẩm
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Index", ((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).TypeOfAccount == 2 ? "User" : "Home", new { area = "Admin" })">
                                <i class="fas fa-users"></i>Tài khoản
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Order", new { area = "Admin" })">
                                <i class="fas fa-table"></i>Đơn hàng
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <div class="page-container">
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">

                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input" type="text" name="search" placeholder="Tìm kiếm..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button2">
                                <div class="account-wrap">
                                    <div class="account-item  js-item-menu">
                                        <div class="image">
                                            <img src="~/Assets/images/icon/avatar-01.jpg" alt="Profile picture" />
                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#">@(((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).Name)</a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix ">
                                                <div class="image">
                                                    <a href="../Home/Account">
                                                        <img src=" /Assets/images/icon/avatar-01.jpg" alt="Profile Picture" />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name">
                                                        @Html.ActionLink(((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).Name.ToString(), "Account", "Home", new { username = ((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).Username.ToString() },null)
                                                    </h5>
                                                    <span class="email"> @(((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).Email) </span>
                                                </div>
                                            </div>
              
                                            <div class="account-dropdown__footer">
                                                @Html.ActionLink("Account", "Account", "Home", new { username = ((Models.EF.User)Session[OnlineShop.Common.CommonConstants.ADMIN_SESSION]).Username.ToString() }, null)
                                            </div>
                                            <div class="account-dropdown__footer">
                                                @Html.ActionLink("Đăng xuất", "Index", "Login")
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </header>
            <div class="main-content">
                @RenderBody()
            </div>
        </div>


    </div>

    <!-- Jquery JS-->
    <script src="/Assets/vendor/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="/Assets/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="/Assets/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!--/Assets/vendor JS       -->
    <script src="/Assets/vendor/slick/slick.min.js">
    </script>
    <script src="/Assets/vendor/wow/wow.min.js"></script>
    <script src="/Assets/vendor/animsition/animsition.min.js"></script>
    <script src="/Assets/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="/Assets/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="/Assets/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="/Assets/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="/Assets/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="/Assets/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="/Assets/vendor/select2/select2.min.js">
    </script>

    <!-- Main JS-->
    <script src="/Assets/js/main.js"></script>
    @RenderSection("js",required:false)

</body>

</html>
<!-- end document-->
